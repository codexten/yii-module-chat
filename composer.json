{
  "name": "codexten/yii-module-chat",
  "type": "library",
  "description": "chat module for yii 2.0 or higher",
  "require": {
    "codexten/yii-web": "~2.0.0",
    "bubasuma/yii2-simplechat": "*"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\modules\\chat\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "core": "config/core.php",
      "moduleChat": "config/moduleChat.php",
      "migrationNamespaces": "config/migrationNamespaces.php"
    }
  }
}
